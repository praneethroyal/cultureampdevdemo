{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Cultureamp-DEV-ADF"
		},
		"LS_for_azureSQLdatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_for_azureSQLdatabase'"
		},
		"LS_for_cultureamp_dev_KV_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Cultureamp-DEV-KV.vault.azure.net/"
		},
		"LS_for_cultureamp_dev_blob_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sec-cultureamp-dev-connetionstring"
		},
		"LS_for_Blob_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sec-cultureamp-dev-connetionstring"
		},
		"LS_for_output_dataset_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sec-cultureamp-dev-connetionstring"
		},
		"LS_for_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Cultureamp-DEV-KV.vault.azure.net/"
		},
		"LS_for_blobstorage_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sec-cultureamp-dev-connetionstring"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Cultureamp-DEV-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_for_azureSQLdatabase_connectionString": {
						"value": "[parameters('LS_for_azureSQLdatabase_connectionString')]"
					},
					"LS_for_cultureamp_dev_KV_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_for_cultureamp_dev_KV_properties_typeProperties_baseUrl')]"
					},
					"LS_for_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_for_keyvault_properties_typeProperties_baseUrl')]"
					},
					"LS_for_cultureamp_dev_blob_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_for_cultureamp_dev_blob_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_for_Blob_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_for_Blob_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_for_output_dataset_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_for_output_dataset_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_for_blobstorage_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_for_blobstorage_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}